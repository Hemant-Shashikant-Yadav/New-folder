{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c11d0357-3e3a-4ba2-b6da-201416507172"
      }
    },
    "android": {
      "package": "com.atharv799.boltexponativewind",
      "kotlinVersion": "1.9.25"
    },
    "owner": "hemantsry"
  }
}
